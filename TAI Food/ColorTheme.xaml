<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TAIFood"
    x:Name="ColorTheme">
    
    <SolidColorBrush x:Key="MainColor" Color="#5D9B14"/>
    <SolidColorBrush x:Key="BackColor" Color="#ECFFD5"/>
    <SolidColorBrush x:Key="SecondaryColor" Color="#AAE861"/>
    <SolidColorBrush x:Key="SubColor" Color="#89269B"/>
    <SolidColorBrush x:Key="SecondarySubColor" Color="#D464E8"/>
    
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Default">
            <SolidColorBrush x:Key="ApplicationPageBackgroundThemeBrush" Color="#ECFFD5" />
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>

</ResourceDictionary>
